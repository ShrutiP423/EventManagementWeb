<!-- ----------------------------------Packages---------------------------------- -->
<section id="package" class="reveal">
    <h2 id="package-heading">OUR PACKAGES</h2>
    <div id="package-main">
        <div class="package-wrapper">

            <i id="package-left" class="fa-solid fa-angle-left"></i>
            <ul class="package-carousel">
                <% if(packages !='' ) { %>
                    <% packages.forEach(function(packages, index){ %>
                        <li class="package-card" id="package-party">
                            <img src="/images/<%= packages.apImg%>" alt="" width="100%" height="120%">
                            <div>
                                <h3><%- packages.pkgname %></h3>
                                <p>
                                <ul>
                                    <%if(packages.bItemList !='' ){%>
                                        <%packages.bItemList.forEach(bItemList=>{%>
                                        <li>
                                            <%=bItemList.basicItem%>
                                        </li>
                                       
                  <%})%>
                  <%}%>
                                </ul><br>
                                <strong>Starting from &#8377 5000</strong></p>
                                <a href="/packages/<%= packages._id%>"><button
                                        class="package-btn">Details</button></a>
                            </div>

                        </li>
       
                        <% }) %>
                            <% } %>
            </ul>
            <i id="package-right" class="fa-solid fa-angle-right"></i>


        </div>

    </div>

</section>
